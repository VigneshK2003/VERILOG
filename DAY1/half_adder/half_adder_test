#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x65015fd657b0 .scope module, "half_adder_tb" "half_adder_tb" 2 1;
 .timescale 0 0;
v0x65015fd78f50_0 .var "a", 0 0;
v0x65015fd79010_0 .var "b", 0 0;
v0x65015fd790e0_0 .net "c", 0 0, L_0x65015fd79410;  1 drivers
v0x65015fd791e0_0 .net "s", 0 0, L_0x65015fd792b0;  1 drivers
S_0x65015fd65940 .scope module, "uut" "half_adder" 2 4, 3 1 0, S_0x65015fd657b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "s";
    .port_info 3 /OUTPUT 1 "c";
L_0x65015fd792b0 .functor XOR 1, v0x65015fd78f50_0, v0x65015fd79010_0, C4<0>, C4<0>;
L_0x65015fd79410 .functor AND 1, v0x65015fd78f50_0, v0x65015fd79010_0, C4<1>, C4<1>;
v0x65015fd65b90_0 .net "a", 0 0, v0x65015fd78f50_0;  1 drivers
v0x65015fd78cb0_0 .net "b", 0 0, v0x65015fd79010_0;  1 drivers
v0x65015fd78d70_0 .net "c", 0 0, L_0x65015fd79410;  alias, 1 drivers
v0x65015fd78e10_0 .net "s", 0 0, L_0x65015fd792b0;  alias, 1 drivers
    .scope S_0x65015fd657b0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x65015fd78f50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x65015fd79010_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x65015fd78f50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x65015fd79010_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x65015fd78f50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x65015fd79010_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x65015fd78f50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x65015fd79010_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 10 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x65015fd657b0;
T_1 ;
    %vpi_call 2 13 "$monitor", "$Time=%0t , a=%b, b=%b ,s =%b, c=%b", $time, v0x65015fd78f50_0, v0x65015fd79010_0, v0x65015fd791e0_0, v0x65015fd790e0_0 {0 0 0};
    %vpi_call 2 14 "$dumpfile", "half_adder.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x65015fd657b0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
